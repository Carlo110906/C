#include <stdio.h>

int main() {
    double calificacion;

    // Entrada de la calificacion
    printf("Introduce la calificacion (0-100): ");
    scanf("%lf", &calificacion);

    // Clasificaci贸n de la calificacion
    if (calificacion >= 90) {
        printf("Calificaci贸n: A\n");
    } else if (calificacion >= 80) {
        printf("Calificaci贸n: B\n");
    } else if (calificacion >= 70) {
        printf("Calificaci贸n: C\n");
    } else if (calificacion >= 60) {
        printf("Calificacion: D\n");
    } else {
        printf("Calificacion: F\n");
    }

    return 0;
}
